        <span class="kt">int</span> <span class="n">KhbfhHtuxwwrn</span> <span class="p">{</span> <span class="k">set</span><span class="p">;</span> <span class="k">get</span><span class="p">;</span> <span class="p">}</span>

        <span class="c1">/// &lt;summary&gt;</span>
        <span class="c1">/// 防御</span>
        <span class="c1">/// &lt;/summary&gt;</span>
        <span class="kt">int</span> <span class="n">KahdxouTrifmznz</span> <span class="p">{</span> <span class="k">set</span><span class="p">;</span> <span class="k">get</span><span class="p">;</span> <span class="p">}</span>

        <span class="c1">/// &lt;summary&gt;</span>
        <span class="c1">/// 精神力</span>
        <span class="c1">/// &lt;/summary&gt;</span>
        <span class="kt">int</span> <span class="n">SnmTiet</span> <span class="p">{</span> <span class="k">get</span><span class="p">;</span> <span class="k">set</span><span class="p">;</span> <span class="p">}</span>

        <span class="c1">/// &lt;summary&gt;</span>
