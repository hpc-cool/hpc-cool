    <span class="p">&lt;</span><span class="n">Grid</span> <span class="n">d</span><span class="p">:</span><span class="n">DataContext</span><span class="p">=</span><span class="s">"{d:DesignInstance viewModel:HnlcDbtdhsdjModel}"</span><span class="p">&gt;</span>
